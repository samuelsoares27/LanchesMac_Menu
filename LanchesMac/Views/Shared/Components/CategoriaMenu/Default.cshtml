<li class="dropdown">
    <a asp-controller="Lanche" asp-action="List" class="dropdown-toggle" data-toggle="dropdown" style="color:white">Lanches   </a>
    <ul class="dropdown-menu">
        @foreach (var categoria in Model)
        {
            <li>
                <a asp-controller="Lanche" asp-action="List" style="color:black"
                   asp-route-categoria="@categoria.CategoriaNome">@categoria.CategoriaNome</a>
            </li>
        }
        <li class="dropdown-divider"></li>
        <li>
            <a asp-controller="Lanche" asp-action="List" asp-route-categoria="" style="color:black">Todos os Lanches</a>
        </li>
    </ul>
</li>

